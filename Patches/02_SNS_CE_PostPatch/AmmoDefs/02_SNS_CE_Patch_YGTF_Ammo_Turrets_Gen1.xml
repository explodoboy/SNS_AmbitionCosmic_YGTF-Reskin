<?xml version="1.0" encoding="utf-8"?>
<Patch>

<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Combat Extended</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Science Never Stops: Ambition of the Cosmic</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
					
					<li Class="PatchOperationReplace">
						<xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_SNS_Ammo_GenI_Turret_Rifle"]/label</xpath>
						<value>
							<label>entropic weapon charge</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_SNS_Ammo_GenI_Turret_Sniper"]/label</xpath>
						<value>
							<label>entropic weapon charge</label>
						</value>
					</li>

					<!-- rifle projectile -->
					<li Class="PatchOperationReplace">
						<xpath>/Defs/ThingDef[defName="SNS_Bullet_GenI_Rifle"]/label</xpath>
						<value>
							<label>light entropic turret projectile</label>
						</value>
					</li>

					<!-- sniper projectile -->
					<li Class="PatchOperationReplace">
						<xpath>/Defs/ThingDef[defName="SNS_Bullet_GenI_Sniper"]/label</xpath>
						<value>
							<label>catalyst turret projectile</label>
						</value>
					</li>
					
				</operations>
			</match>
			</li>
		</operations>
	</match>
</Operation>

</Patch>